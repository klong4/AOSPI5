# Raspberry Pi 5 SELinux Property Contexts

# Device properties
ro.hardware.                 u:object_r:default_prop:s0
ro.board.                    u:object_r:default_prop:s0
ro.boot.hardware             u:object_r:default_prop:s0

# USB properties
sys.usb.                     u:object_r:system_prop:s0
sys.usb.config               u:object_r:system_prop:s0
sys.usb.configfs             u:object_r:system_prop:s0
sys.usb.controller           u:object_r:system_prop:s0
sys.usb.ffs.ready            u:object_r:system_prop:s0
sys.usb.state                u:object_r:system_prop:s0

# Bluetooth properties
bluetooth.                   u:object_r:bluetooth_prop:s0
persist.bluetooth.           u:object_r:bluetooth_prop:s0

# WiFi properties
wifi.                        u:object_r:wifi_prop:s0
wlan.                        u:object_r:wifi_prop:s0

# Power management
persist.sys.power.           u:object_r:system_prop:s0

# Display properties
persist.sys.sf.              u:object_r:system_prop:s0
ro.surface_flinger.          u:object_r:default_prop:s0

# GPU properties
ro.hardware.vulkan           u:object_r:default_prop:s0
ro.hardware.egl              u:object_r:default_prop:s0
